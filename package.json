{
  "name": "flyer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "run-p -r dev:vite dev:wrangler",
    "dev:vite": "vite dev --host",
    "dev:wrangler": "wrangler dev node_modules/wrangler-proxy/dist/worker.js",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "migrate:remote": "dotenv cross-var 'wrangler d1 migrations apply $D1_DATABASE_NAME --remote'",
    "migrate:local": "dotenv cross-var 'wrangler d1 migrations apply $D1_DATABASE_NAME --local'",
    "gen:migration": "drizzle-kit generate:sqlite --out migrations --schema src/schema.ts",
    "gen:wrangler.toml": "envsub --all --env-file .env wrangler.base.toml wrangler.toml"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240423.0",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-cloudflare": "^4.4.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.16",
    "better-sqlite3": "^9.6.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "^0.20.17",
    "envsub": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "wrangler": "^3.53.0",
    "wrangler-proxy": "^2.2.7"
  },
  "type": "module",
  "dependencies": {
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.30.10",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1"
  },
  "packageManager": "yarn@4.1.1"
}
